# The operating service mode of Orion system.
# Reserved values can be used in future revisions of the specification.

uint8 value

uint8 NONE          = 0
uint8 COLLIMATOR    = 1 # Laser range finder
uint8 MRF           = 2 # Manual range finder
uint8 GUN_MAIN      = 3 # Main gun ballistics
uint8 GUN_SECOND    = 4 # Second gun ballistics

@sealed
